<p>
  <strong>Name</strong>
  <%= @test.name %>
</p>

<p>
  <strong>Level</strong>
  <%= @test.level %>
</p.
<% @test.questions.each do |question| %>
  <tr>
    <th><%=question.text%></th>
  </tr>
<% end %>
<ul>
  <li><%= link_to "Edit", edit_test_path(@test) %></li>
  <li><%= link_to "Destroy", test_path(@test),
                  method: :delete,
                  data: {confirm: "Are you sure"}%></li>
</ul>
